datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

generator typegraphql {
  provider = "typegraphql-prisma"
  output = "../graphql/type-graphql"
}

model Users {
  userID Int @id @default(autoincrement()) @map("UserID")
  firstName String @map("FirstName")
}
